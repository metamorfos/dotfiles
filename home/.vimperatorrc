set "editor=/usr/local/bin/mvim -f"
set gui=navigation,nobookmarks,nomenu,noaddons
nnoremap ; :

set incsearch 
set hlsearch
set ignorecase
nnoremap ,, :nohlsearch<cr> 
nmap gI 2gi

" move faster
nnoremap j 3j
nnoremap k 3k

nnoremap H :back<CR>
nnoremap L :forward<CR>

noremap <M-l> :open 
inoremap <M-l> <esc>:open 
cnoremap <M-l> <esc>:open 
noremap <M-f> /
inoremap <M-f> <esc>/
cnoremap <M-f> <esc>/
noremap <M-k> :open google 
noremap <M-t> :tabnew 
inoremap <M-t> <esc>:tabnew 
cnoremap <M-t> <esc>:tabnew 

noremap <leader>f /
noremap <leader>r :source ~/.vimperatorrc<CR>

