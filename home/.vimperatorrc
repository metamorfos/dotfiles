set "editor=/usr/local/bin/mvim -f"
nnoremap ; :
let mapleader = ","

set incsearch 
set hlsearch
set ignorecase
nnoremap <leader><space> :noh<cr> 

" move faster
nnoremap j 3j
nnoremap k 3k

nnoremap H :back<CR>
nnoremap L :forward<CR>

noremap <M-l> :open 
noremap <M-f> /
noremap <M-k> :open google 
noremap <M-t> :tabnew 

noremap <leader>f /
noremap <leader>r :source ~/.vimperatorrc<CR>
