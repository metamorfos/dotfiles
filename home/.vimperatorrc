set "editor=/usr/local/bin/mvim -f"
nnoremap ; :
set mapleader=','

set incsearch 
set hlfind
set ignorecase
nnoremap <leader><space> :noh<cr> 

" move faster
nnoremap j 3j
nnoremap k 3k

nnoremap H :back<CR>
nnoremap L :forward<CR>
nnoremap J :tabp<CR>
nnoremap K :tabn<CR>

noremap <M-l> :open 
noremap <M-f> /
noremap <M-k> :open google 
noremap <M-h> :open google.com<CR>
noremap <M-t> :tabnew 
