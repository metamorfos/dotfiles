colo solarized
set background=light
set anti
set guifont=Inconsolata-dz:h13
set laststatus=2
set lines=45 columns=120
set go-=T
set go-=l
set go-=L
set go-=r
set go-=R
set go-=e

" M-# switches to tab
noremap <d-1> 1gt
noremap <d-2> 2gt
noremap <d-3> 3gt
noremap <d-4> 4gt
noremap <d-5> 5gt
noremap <d-6> 6gt
noremap <d-7> 7gt
noremap <d-8> 8gt
noremap <d-9> 9gt
